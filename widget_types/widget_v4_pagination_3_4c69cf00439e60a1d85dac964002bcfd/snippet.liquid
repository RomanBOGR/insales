{% if paginate.pages > 1 %}
<div class="pagination">
  {% if paginate.previous %}
  <a href="{{ paginate.previous.url }}" class="button button_size-m pagination-prev icon icon-arrow-left"></a>
  {% endif %}
  <div class="pagination-items">
    {% for part in paginate.parts %}
    {% if part.title == paginate['current_page'] %}
    <span class="button button_size-m pagination-span disabled">
    {{ part.title }}
    </span>
    {% else %}
    {% if part.is_link %}
    <a href="{{ part.url }}" class="button button_size-m  pagination-link">
    {{ part.title }}
    </a>
    {% else %}
    <span class=" pagination-void">
    {{ part.title }}
    </span>
    {% endif %}
    {% endif %}
    {% endfor %}
  </div>
  {% if paginate.next %}
  <a href="{{ paginate.next.url }}" class="button button_size-m pagination-next icon icon-arrow-right"></a>
  {% endif %}
</div>

{% endif %}